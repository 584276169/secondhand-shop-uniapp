<view><view style="margin-bottom:250rpx;"><view class="form_addArticle"><text data-event-opts="{{[['tap',[['publish',['$event']]]]]}}" bindtap="__e">发表文章</text></view><block wx:for="{{list}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['Godetail',['$0'],[[['list','',index,'id']]]]]]]}}" class="container forum_content" bindtap="__e"><view class="flex forum_article"><image src="{{item.avatarUrl}}" mode="aspectFill"></image><view class="article_user flex_column"><text class="user_text">{{item.nickName}}</text><text>{{item.time}}</text></view></view><view class="article_text"><text user-select="{{true}}">{{item.text}}</text></view><view><u-album vue-id="{{'e56e75c8-1-'+index}}" urls="{{item.img}}" previewFullImage="{{false}}" bind:__l="__l"></u-album></view><view class="article_btn flex_around"><block wx:for="{{btn_logo}}" wx:for-item="child" wx:for-index="indexOI" wx:key="indexOI"><view data-event-opts="{{[['tap',[['seedetail',['$0','$1',index,'$2'],[[['btn_logo','',indexOI,'id']],[['list','',index,'id']],[['list','',index]]]]]]]}}" class="btn_content" catchtap="__e"><block wx:if="{{child.id===0}}"><view class="btn_tap"><button type="default" open-type="share"><image src="{{child.icon}}" mode="aspectFill"></image></button></view></block><block wx:else><view class="btn_tap"><image src="{{child.icon}}" mode="aspectFill"></image><text hidden="{{!(child.liked)}}">{{item.liked}}</text></view></block></view></block></view></view></block></view><tabbar vue-id="e56e75c8-2" tab="{{1}}" bind:__l="__l"></tabbar><show-modal vue-id="e56e75c8-3" bind:__l="__l"></show-modal></view>